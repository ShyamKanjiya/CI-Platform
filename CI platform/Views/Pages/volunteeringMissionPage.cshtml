                                @if (User.Identity.IsAuthenticated)
                                {
                                    <form class="mt-4">
                                        <textarea class="form-control" name="" id="comment_area" cols="30" rows="4" placeholder="Enter your comments.."></textarea>
                                    </form>

                                    <button class="btn btn-outline-danger rounded-pill m-3 ms-0 " onclick="AddComment(@Model.MissionDetail.MissionId)">Post comments</button>
                                }
                                @foreach (var item in Model.commentList)
                                {
                                    DateTime dt = @item.CreatedAt;
                                    <div class="container w-100  border bg-light" style="overflow: auto;">
                                        <div style="height: 26rem;" class="row">
                                            <div class="col-12 bg-white mt-3 mb-3 pt-2">
                                                <div class="container-fluid">
                                                    <div class="row">
                                                        <div class="col-auto my-auto">
